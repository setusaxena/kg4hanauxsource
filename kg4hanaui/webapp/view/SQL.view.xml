<mvc:View controllerName="com.sap.kg4hana.kg4hanaui.controller.SQL" xmlns:l="sap.ui.layout"
	xmlns:layout="sap.suite.ui.commons.networkgraph.layout" xmlns:core="sap.ui.core" xmlns:networkgraph="sap.suite.ui.commons.networkgraph"
	xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m" height="100%">
	<App>
		<pages>
			<Page title="SQL Query Page" id="pageSQL">
				<content>
					<Button text="Go Back" press="onNavigateBack"></Button>
					<VBox class="sapUiMediumMargin">
						<Title titleStyle="H4" text="{oObjectSqlModel>/Title}" class="sapUiSmallMargin"/>
						<Text text="{oObjectSqlModel>/URI}"/>
					</VBox>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>